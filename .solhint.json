{
    "extends": "solhint:all",
    "rules": {
      "indent": ["error", 4],
      "compiler-fixed": "off",
      "contract-name-camelcase": "off",
      "const-name-snakecase": "off",
      "no-simple-event-func-name": "off",
      "no-empty-blocks": "off",
      "not-rely-on-time": "off",
      "two-lines-top-level-separator": "off",
      "mark-callable-contracts": "off",
      "reason-string": "off"
    }
  }
